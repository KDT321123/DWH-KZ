{
  "name": "@sap/generator-fiori-elements",
  "displayName": "SAP Fiori elements application (Deprecated)",
  "version": "1.6.6",
  "description": "This generator has been deprecated. Please use the SAP Fiori generator that combines SAP Fiori freestyle and SAP Fiori elements generators in one.",
  "homepage": "http://www.sapfiorielements.com/",
  "main": "generators/app/index.js",
  "license": "SEE LICENSE IN LICENSE",
  "author": "SAP SE",
  "keywords": [
    "SAP",
    "fiori elements",
    "fiori generator"
  ],
  "private": true,
  "generator-filter": {
    "types": [
      "project"
    ]
  },
  "scripts": {
    "start": "node generators/index.js",
    "pre-commit": "lint-staged --quiet",
    "clean:dist": "rimraf ./dist ./generators *.tsbuildinfo",
    "clean": "rimraf ./typings ./reports ./test/test-output",
    "watch": "yarn clean &&  tsc --build ./ -w",
    "build:compile": "tsc --build ./ --pretty  ",
    "build": "npm-run-all clean build:compile",
    "build:deps": "yarn lerna run --concurrency 1 --no-bail --stream --scope @sap/ux-cds --scope @sap/ux-generator-common --scope @sap/generator-fiori-deployment --scope @sap/ux-odata-client --scope @sap/ux-project-inquirer --scope @sap/ux-service-inquirer --scope @sap/ux-project-generator --scope @sap/ux-telemetry --scope @sap/ux-ui5-info --scope @sap/ux-common-utils --scope @sap/ux-store build",
    "bundle": "yarn build:deps && yarn build && npm pack",
    "build:dev": "yarn build:deps && yarn build",
    "bundle:dev": "yarn build:dev && npm pack",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:summary": "eslint . --ext .ts,.tsx -f summary",
    "lint:fix": "eslint --fix",
    "lint:fix:all": "eslint . --ext .ts,.tsx --fix",
    "lint:report": "eslint . --ext .ts,.tsx -f multiple ",
    "format:fix": "prettier --write --loglevel silent --ignore-path ../../../.prettierignore",
    "format:fix:all": "prettier --write '**/*.{css,scss,html,js,json,ts,tsx,yaml,yml}' '!**/{out,typings,node_modules}/**' '!**/*.{svg,png,xml}' '!resources/templates/**' --ignore-path ../../../.prettierignore",
    "test": "jest --ci --maxWorkers=1 --detectOpenHandles --forceExit --colors",
    "prepack": "rimraf node_modules/@sap/*/node_modules/@*",
    "tgz:package": "npm pack",
    "madge": "madge --warning --circular --extensions ts ./"
  },
  "dependencies": {
    "@sap-ux/fiori-elements-writer": "0.3.6",
    "@sap/cds": "5.9.7",
    "express": "4.17.1",
    "json-parse-better-errors": "1.0.2",
    "json-parse-even-better-errors": "2.3.1",
    "uuid": "8.3.2",
    "yeoman-generator": "5.4.2"
  },
  "optionalDependencies": {
    "keytar": "7.8.0"
  },
  "devDependencies": {
    "@sap-devx/yeoman-ui-types": "1.0.2",
    "@sap-ux/edmx-parser": "0.5.4",
    "@sap/ux-annotations": "1.6.6",
    "@sap/ux-cds": "1.6.6",
    "@sap/ux-common-utils": "1.6.6",
    "@sap/ux-feature-toggle": "1.6.6",
    "@sap/ux-generator-common": "1.6.6",
    "@sap/ux-jest-matchers": "0.2.6",
    "@sap/ux-odata-client": "1.6.6",
    "@sap/ux-project-access": "1.6.6",
    "@sap/ux-project-generator": "1.6.6",
    "@sap/ux-project-inquirer": "1.6.6",
    "@sap/ux-service-inquirer": "1.6.6",
    "@sap/ux-telemetry": "1.6.6",
    "@sap/ux-ui5-info": "1.6.6",
    "@sap/wing-service-explorer": "1.7.2",
    "@types/yeoman-environment": "2.10.5",
    "@types/yeoman-generator": "5.2.7",
    "copyfiles": "2.1.1",
    "dir-compare": "2.3.0",
    "fast-xml-parser": "3.12.20",
    "i18next": "^19.4.4",
    "inquirer": "8.2.0",
    "inquirer-search-list": "1.2.6",
    "lodash": "4.17.21",
    "mem-fs-editor": "9.3.0",
    "mock-spawn": "^0.2.6",
    "prettify-xml": "1.2.0",
    "semver-compare": "1.0.0",
    "source-map-loader": "3.0.0",
    "ts-loader": "9.2.3",
    "typescript": "3.8.3",
    "validate-npm-package-name": "3.0.0",
    "yeoman-environment": "3.8.0",
    "yeoman-test": "6.2.0",
    "yo": "4.3.0"
  },
  "config": {
    "unsafe-perm": true
  },
  "files": [
    "generators/",
    "templates/",
    "yeoman.png"
  ],
  "jestSonar": {
    "reportPath": "reports/test/unit",
    "reportFile": "test-report.xml"
  },
  "eslint-formatter-multiple": {
    "formatters": [
      {
        "name": "stylish",
        "output": "console"
      },
      {
        "name": "json",
        "output": "file",
        "path": "reports/lint/eslint.json"
      },
      {
        "name": "checkstyle",
        "output": "file",
        "path": "reports/lint/eslint.checkstyle.xml"
      }
    ]
  }
}
