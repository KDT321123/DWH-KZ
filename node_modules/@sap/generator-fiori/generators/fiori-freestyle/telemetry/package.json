{
  "name": "@sap/generator-fiori-freestyle",
  "version": "1.6.6",
  "displayName": "SAP Fiori freestyle SAPUI5 application (Deprecated)",
  "description": "This generator has been deprecated. Please use the SAP Fiori generator that combines SAP Fiori freestyle and SAP Fiori elements generators in one.",
  "homepage": "https://ui5.sap.com/",
  "main": "generators/app/index.js",
  "generator-filter": {
    "types": [
      "project"
    ]
  },
  "scripts": {
    "start": "node generators/app/index.js",
    "clean:dist": "rimraf ./dist ./generators *.tsbuildinfo",
    "clean": "rimraf ./typings ./reports ./test-output",
    "watch": "yarn clean && tsc --build ./ -w",
    "build:compile": "tsc --build ./ --pretty ",
    "build": "npm-run-all clean build:compile",
    "build:deps": "yarn lerna run --concurrency 1 --no-bail --stream --scope @sap/ux-cds --scope @sap/ux-generator-common --scope @sap/generator-fiori-deployment --scope @sap/ux-odata-client --scope @sap/ux-service-inquirer --scope @sap/ux-project-inquirer --scope @sap/ux-project-generator --scope @sap/ux-telemetry build",
    "bundle": "yarn build:deps && yarn build && npm pack",
    "build:dev": "yarn build:deps && yarn build",
    "bundle:dev": "yarn build:dev && npm pack",
    "build:dev-vscode": "yarn clean && tsc --build ./",
    "test:app": "node -r ts-node/register test/app.ts",
    "test:ui5": "node -r ts-node/register test/ui5.ts",
    "lint": "eslint . --ext .ts",
    "lint:summary": "eslint . --ext .ts -f summary",
    "lint:fix": "eslint --fix",
    "lint:fix:all": "eslint . --ext .ts --fix",
    "lint:report": "eslint . --ext .ts -f multiple ",
    "prepack": "rimraf node_modules/@sap/*/node_modules/@*",
    "yo": "mkdir -pv temp && cd temp && yo ../generators/app && cd ..",
    "test": "jest --maxWorkers=2 --ci --forceExit --detectOpenHandles",
    "tgz:package": "npm pack",
    "madge": "madge --warning --circular --extensions ts ./"
  },
  "license": "SEE LICENSE IN LICENSE",
  "author": "SAP SE",
  "keywords": [
    "SAP",
    "fiori freestyle",
    "fiori generator"
  ],
  "dependencies": {
    "@sap-ux/fiori-freestyle-writer": "0.11.15",
    "@sap/cds": "5.9.7",
    "express": "4.17.1",
    "handlebars": "4.7.7",
    "lodash": "4.17.21",
    "yaml": "2.0.0-10",
    "yeoman-generator": "5.4.2"
  },
  "optionalDependencies": {
    "keytar": "7.8.0"
  },
  "devDependencies": {
    "@sap-devx/yeoman-ui-types": "1.0.2",
    "@sap/ux-feature-toggle": "1.6.6",
    "@sap/ux-generator-common": "1.6.6",
    "@sap/ux-jest-matchers": "0.2.6",
    "@sap/ux-odata-client": "1.6.6",
    "@sap/ux-project-generator": "1.6.6",
    "@sap/ux-project-inquirer": "1.6.6",
    "@sap/ux-service-inquirer": "1.6.6",
    "@sap/ux-telemetry": "1.6.6",
    "@sap/ux-ui5-info": "1.6.6",
    "@sap/wing-service-explorer": "1.7.2",
    "@types/lodash": "4.14.175",
    "@types/yeoman-environment": "2.10.5",
    "@types/yeoman-generator": "5.2.7",
    "adm-zip": "0.4.14",
    "express": "4.17.1",
    "inquirer": "8.2.0",
    "inquirer-search-list": "1.2.6",
    "source-map-loader": "3.0.0",
    "ts-loader": "9.2.3",
    "typescript": "3.8.3",
    "yeoman-test": "6.2.0",
    "yo": "4.3.0"
  },
  "files": [
    "generators",
    "templates",
    "yeoman.png"
  ],
  "jestSonar": {
    "reportPath": "reports/test/unit",
    "reportFile": "test-report.xml"
  },
  "eslint-formatter-multiple": {
    "formatters": [
      {
        "name": "stylish",
        "output": "console"
      },
      {
        "name": "json",
        "output": "file",
        "path": "reports/lint/eslint.json"
      },
      {
        "name": "checkstyle",
        "output": "file",
        "path": "reports/lint/eslint.checkstyle.xml"
      }
    ]
  }
}
