{
  "name": "@sap/bas-sdk",
  "description": "SDK for SAP Business Application Studio",
  "version": "2.1.5",
  "main": "dist/src/index.js",
  "license": "SAP",
  "typings": "./api.d.ts",
  "files": [
    "dist",
    "src",
    "api.d.ts"
  ],
  "scripts": {
    "ci": "npm-run-all clean compile coverage",
    "ci:artifacts_only": "npm-run-all compile",
    "clean": "rimraf ./coverage ./docs ./nyc_output ./dist",
    "compile": "tsc",
    "test": "mocha ",
