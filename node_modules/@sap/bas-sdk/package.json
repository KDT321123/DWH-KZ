    "doc:gen": "typedoc --excludeExternals --out ../../docs/ ./api.d.ts"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "axios": "0.21.4",
    "cross-spawn": "^7.0.3",
    "lodash": "4.17.21",
    "url-join": "^4.0.1",
    "ws": "^8.5.0"
  },
  "devDependencies": {
    "@sap/bas-sdk-sinon-helper": "^2.1.5",
    "@types/cross-spawn": "^6.0.2",
    "@types/lodash": "^4.14.182",
    "@types/url-join": "^4.0.0",
    "@types/uuid": "8.3.0",
    "@types/ws": "^8.5.2",
    "axios-mock-adapter": "^1.19.0"
  }
}
