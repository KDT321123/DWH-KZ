PROCEDURE "kz.Test.Delta::B_Case_Delta_Pa"( IN in_delta_time TIMESTAMP DEFAULT NULL,
                                            OUT out_tab "kz.DI.ERP::V_B_Case",
                                            OUT out_delta_time TIMESTAMP
                                          )
   LANGUAGE SQLSCRIPT
   SQL SECURITY INVOKER
   --DEFAULT SCHEMA "DWH_HDI_DB_1"
   --READS SQL DATA 
   AS
BEGIN
  
  out_tab = SELECT * FROM "kz.DI.ERP::V_B_Case" WHERE "B_Case_createTime" > :in_delta_time;
  SELECT MAX("LAST_TIMESTAMP") INTO out_delta_time FROM "kz.Test.Delta::DeltaLog";
  
END